{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "navin",
  "version": "0.1.0",
  "identifier": "com.vp739.navin",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Navin - Developer Checklist",
        "width": 1200,
        "height": 800,
        "minWidth": 320,
        "minHeight": 200,
        "resizable": true,
        "fullscreen": false,
        "decorations": true,
        "transparent": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "sql": {
      "default": {
        "path": "navin.db"
      }
    }
  }
}
